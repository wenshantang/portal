<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>fhcf.portal</display-name>
  <filter>
    <filter-name>Puff</filter-name>
    <filter-class>com.puff.web.mvc.PuffFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Puff</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <context-param>
    <param-name>start.config</param-name>
    <param-value>cn.aresoft.cms.portal.fhcf.StartCD</param-value>
  </context-param>
  <error-page>
    <error-code>404</error-code>
    <location>/WEB-INF/error/404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/WEB-INF/error/500.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.Exception</exception-type>
    <location>/WEB-INF/error/500.jsp</location>
  </error-page>
</web-app>