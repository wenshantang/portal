.menuhover { display:block; text-decoration:none; font-size:22px; color:#fff; text-align:center;letter-spacing:3px; font-weight:300; background:#e46c0a;}

.menu{width:380px; height:220px; padding-top:20px; background:#F6F3ED;opacity:0.85; border-top:2px solid #e88737;}
.menu span{ width:150px; font-size:14px; color:#333; display:block; float:left; line-height:25px; padding:10px 10px 10px 30px; text-align:left;}
.menu span a{ width:120px; font-size:14px; color:#e46c0a; display:block; float:left;padding:5px 10px; text-align:center; border:1px solid #e46c0a; background:#FFF; opacity:0.9;border-radius:5px; margin-top:20px;}
.menu ul{ width:180px; float:right; margin-top:5px;}
.menu ul li{ border-bottom:2px solid #e3e0d9; border-right:0px; width:160px; font-size:16px; color:#666; text-align:center;}
